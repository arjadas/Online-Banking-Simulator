datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Transaction {
  id    Int     @id @default(autoincrement())
  amount String  @unique
  reference  String
  description String?
}
